#include <stdio.h>
#include <stdlib.h>

#include "ims/Store.hh"
#include "Remaining.hh"

static const char USAGE[] = "usage: ims_remaining partition\n";

#define PROGRAM   argv[0]
#define PARTITION argv[1]

/*
** ++
**
**
** --
*/
static const char folder[]="";

int main(int argc, char* argv[])
{
  
  if(argc < 2) {printf(USAGE); return EXIT_SUCCESS;}

  IMS::Store store(PARTITION);
  IMS::Editor::Remaining remaining(store);
  remaining.process(0, (const char**)0, 0);
  return EXIT_SUCCESS;
}
